@{
    ViewBag.Title = "Calendar";
}

<html>
    <head>
        <title>@ViewBag.Title</title>
        
        <script type="text/javascript">
            $(document).ready(function () {
                $('#calendar').fullCalendar({
                    weekends: false
                });
            });
        </script>
    </head>
    <body>
        <div id='calendar'>
            
        </div>
    </body>
</html>